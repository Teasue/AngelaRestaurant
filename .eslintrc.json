{
    "extends": "airbnb",
    "plugins": [
        "promise"
    ],
    "rules":{
        "linebreak-style":"off",
        "consistent-return":"off",
        "no-return-assign":"off",
        "no-var":"off",
        "img-has-alt":"off",
        "curly":"off",
        "prefer-const":"off",
        "vars-on-top":"off",
        "no-plusplus":"off",
        "no-continue":"off"

    }
}